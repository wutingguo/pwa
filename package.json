  "betterScripts": {
    "dev:pc": {
      "command": "concurrently --kill-others --kill-others-on-fail -p name --names \"config,common-deps,navbar,software\" \"npm run dev:config\" \"npm run dev:common-deps\" \"npm run dev:navbar\" \"npm run dev:software\" \"npm run dev:saas-portal-zno\" ",
      "env": {
        "PLANTFORM": "pc"
      }
    },
    "dev:nocloud:pc": {
      "command": "concurrently --kill-others --kill-others-on-fail -p name --names \"config,common-deps,navbar,software\" \"npm run dev:config\" \"npm run dev:common-deps\" \"npm run dev:navbar\" \"npm run dev:software\"",
