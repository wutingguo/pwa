      "env": {
        "PLANTFORM": "wap"
      }
    },
    "build:pc": {
      "command": "npm run build:config && npm run build:common-deps && npm run build:navbar && npm run build:software && npm run build:saas-portal-zno && npm run clear-version && npm run build:done",
      "env": {
        "PLANTFORM": "pc"
      }
    },
    "build:wap": {
      "command": "npm run build:config && npm run build:common-deps && npm run build:navbar && npm run build:software && npm run build:saas-portal-zno && npm run clear-version && npm run build:done",
      "env": {
        "PLANTFORM": "wap"
      }
    },
    "dev:cn:pc": {
      "command": "concurrently --kill-others --kill-others-on-fail -p name --names \"config,common-deps,navbar,software\" \"npm run dev:config:cn\" \"npm run dev:common-deps:cn\" \"npm run dev:navbar:cn\" \"npm run dev:software:cn\" \"npm run dev:saas-portal:cn\"",
      "env": {
        "PLANTFORM": "pc"
      }
    },
    "dev:cn:wap": {
      "command": "concurrently --kill-others --kill-others-on-fail -p name --names \"config,common-deps,navbar,software\" \"npm run dev:config:cn\" \"npm run dev:common-deps:cn\" \"npm run dev:navbar:cn\" \"npm run dev:software:cn\" \"npm run dev:saas-portal:cn\"",
      "env": {
        "PLANTFORM": "wap"
      }
    },
    "build:cn:pc": {
      "command": "npm run clear-version && npm run build:config:cn && npm run build:common-deps:cn && npm run build:navbar:cn && npm run build:software:cn && npm run build:saas-portal:cn && npm run build:done",
      "env": {
        "PLANTFORM": "pc"
      }
    },
    "build:cn:wap": {
      "command": "npm run clear-version && npm run build:cn:dashboard-mobile && npm run build:saas-portal:cn && npm run build:done",
      "env": {
        "PLANTFORM": "wap"
      }
    }
