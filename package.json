  "description": "",
  "main": "index.js",
  "scripts": {
    "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "node postinstall.js",
    "update:dep": "node dep.js",
    "clean": "rimraf ./build/*",
    "clear-version": "node run.js --env.project=clearVersion",
    "install-all": "npm run install:config && npm run install:navbar && npm run install:software && npm run install:saas-portal",
    "install:config": "npm i",
    "install:navbar": "cd ./navbar/ && npm i",
    "install:software": "cd ./software/ && npm i",
    "install:designer": "cd ./designer/ && npm i",
    "install:saas-portal": "cd ./saas-portal/ && npm i",
    "predev:pc": "npm run clean",
    "predev:wap": "npm run update & npm run clean",
    "dev:pc": "better-npm-run dev:pc",
    "dev:noCloud:pc": "better-npm-run dev:nocloud:pc",
    "dev:wap": "better-npm-run dev:wap",
    "dev:noCloud:wap": "better-npm-run dev:nocloud:wap",
    "predev:cn:pc": "npm run update:cn & npm run clean",
    "predev:cn:wap": "npm run update:cn & npm run clean",
    "dev:cn:pc": "better-npm-run dev:cn:pc",
    "dev:cn:wap": "better-npm-run dev:cn:wap",
    "dev:config": "node run.js --env.project=config --env.port=8233",
    "dev:config:cn": "node run.js --env.project=config --env.port=8233 --env.cn",
    "dev:common-deps": "node run.js --env.project=common-deps --env.port=8234",
